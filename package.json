{
  "name": "PowerTracking",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clear": "sh ./clear.sh",
    "start": "npm run clear && npx react-native start --reset-cache",
    "android": "npx react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "sh ./postinstall.sh",
    "shake": "adb shell input keyevent 82",
    "dev": "ENVFILE=.env && npx react-native run-android",
    "attach-android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "del-android-bundle": "rm -f ./android/app/src/main/assets/index.android.bundle",
    "del-apk-file": "rm -f ./android/app/build/outputs/apk/debug/*.apk && rm -rf ./android/app/build/outputs/apk/release/*.apk",
    "prepare-android-bundle": "npm run clear && npm run del-android-bundle && npm run attach-android-bundle",
    "rm-dup-files": "find android/app/src/main/res/raw android/app/src/main/res/drawable-xxxhdpi android/app/src/main/res/drawable-xxhdpi android/app/src/main/res/drawable-xhdpi android/app/src/main/res/drawable-mdpi android/app/src/main/res/drawable-hdpi -type f ! -name launch_screen* -delete",
    "release-apk:stg": "npm run prepare-android-bundle && npm run del-apk-file && npm run rm-dup-files && cd ./android && ENVFILE=.env.staging ./gradlew assembleRelease"
  },
  "dependencies": {
